{
    "hosting": {
        "public": "build/web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
                "headers": [
                    {
                        "key": "Access-Control-Allow-Origin",
                        "value": "*"
                    }
                ]
            },
            {
                "source": "**/*.@(js|css)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=604800"
                    }
                ]
            },
            {
                "source": "**/*.@(jpg|jpeg|gif|png)",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=604800"
                    }
                ]
            }
        ]
    },
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "cateyepos",
                    "appId": "1:765053941387:android:e8f05ec3c1f40bbceb468e",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "cateyepos",
                    "configurations": {
                        "android": "1:765053941387:android:e8f05ec3c1f40bbceb468e",
                        "ios": "1:765053941387:ios:dd4fd0551a4ebb98eb468e",
                        "macos": "1:765053941387:ios:dd4fd0551a4ebb98eb468e",
                        "web": "1:765053941387:web:c32b1e7c56177e61eb468e",
                        "windows": "1:765053941387:web:b17aef78db2ebc05eb468e"
                    }
                }
            }
        }
    }
}